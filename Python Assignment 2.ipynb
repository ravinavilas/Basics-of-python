    "#Question 1\n",
    "def rev(Inp):\n",
    " ReverseInp=Inp[::-1] # Slicing\n",
    " print (\"The Reversed String is\",ReverseInp) # Print the reversed string\n",
    "\n",
    "Stri=input(\"Enter the String to be Reversed \")\n",
    "rev(Stri)"
